    <p style="color: #0A1538;">{{form_label(form.matriculeFiscale)}}</p>
    <p style="margin-left: 290px;margin-top: -20px;">{{form_widget(form.matriculeFiscale)}}</p>

    <p style="color: #0A1538;">{{form_label(form.registreDesCommeerces)}}</p>
